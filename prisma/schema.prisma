generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               String            @id
  email            String            @unique
  firstName        String?
  lastName         String?
  phone            String?
  photo            String?
  gender           String?           @default("na")
  createdAt        DateTime          @default(now())
  updatedAt        DateTime          @updatedAt
}
